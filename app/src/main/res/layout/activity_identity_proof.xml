<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="identityProofVM"
            type="com.kirtikumarsapplication.app.modules.identity_proof.data.viewmodel.IdentityProofVM" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/save_as_draft_btn"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:id="@+id/linearAndroidLarge"
                style="@style/groupStylewhite_A700"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="start"
                android:orientation="vertical">

                <FrameLayout
                    android:id="@+id/frameStack"
                    style="@style/groupStylewhite_1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_62pxv"
                    android:layout_marginTop="@dimen/_12pxv">

                    <ImageView
                        android:id="@+id/imageImage726"
                        android:layout_width="@dimen/_120pxh"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_image726"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_image726" />
                </FrameLayout>

                <LinearLayout
                    android:id="@+id/linearColumnOne"
                    style="@style/groupStylecornerRadius"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearColumnidentityproof"
                        style="@style/groupStylecornerRadius"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtIdentityProof"
                            style="@style/txtIntersemibold14"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:text="@{identityProofVM.identityProofModel.txtIdentityProof}"
                            tools:text="@string/lbl_identity_proof" />

                        <FrameLayout
                            android:id="@+id/frameStackgroup18606"
                            android:layout_width="@dimen/_320pxh"
                            android:layout_height="@dimen/_8pxv"
                            android:layout_gravity="start"
                            android:layout_marginTop="@dimen/_13pxv">

                            <LinearLayout
                                android:id="@+id/linearColumn"
                                style="@style/groupStylecornerRadius_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:id="@+id/linearColumngroup18606"
                                    style="@style/groupStylecornerRadius_1"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical">

                                    <ProgressBar
                                        android:id="@+id/progressBarGroup18606"
                                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:gravity="start"
                                        android:indeterminate="false"
                                        android:max="100"
                                        android:minHeight="@dimen/_8pxv"
                                        android:progress="33"
                                        android:progressDrawable="@drawable/layer_list_radius_4" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linearColumnrectangle30405"
                                style="@style/groupStyledeep_purple_900_75cornerRadius"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:orientation="vertical">

                                <View
                                    android:id="@+id/viewRectangle30405"
                                    android:layout_width="@dimen/_8pxh"
                                    android:layout_height="@dimen/_8pxh"
                                    android:layout_gravity="start"
                                    android:background="@drawable/rectangle_bg_deep_purple_900_75_radius_4" />
                            </LinearLayout>

                            <View
                                android:id="@+id/viewRectangle5396"
                                android:layout_width="@dimen/_8pxh"
                                android:layout_height="@dimen/_8pxh"
                                android:layout_gravity="end"
                                android:layout_marginStart="@dimen/_125pxh"
                                android:layout_marginEnd="@dimen/_125pxh"
                                android:background="@drawable/rectangle_bg_deep_purple_900_75_radius_4" />

                            <View
                                android:id="@+id/viewRectangle30403"
                                android:layout_width="@dimen/_8pxh"
                                android:layout_height="@dimen/_8pxh"
                                android:layout_gravity="end"
                                android:layout_marginStart="@dimen/_62pxh"
                                android:layout_marginEnd="@dimen/_62pxh"
                                android:background="@drawable/rectangle_bg_deep_purple_900_75_radius_4" />

                            <View
                                android:id="@+id/viewRectangle30404"
                                android:layout_width="@dimen/_8pxh"
                                android:layout_height="@dimen/_8pxh"
                                android:layout_gravity="end"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginEnd="@dimen/_1pxh"
                                android:background="@drawable/rectangle_bg_deep_purple_900_75_radius_4" />

                            <View
                                android:id="@+id/viewRectangle5398"
                                android:layout_width="@dimen/_8pxh"
                                android:layout_height="@dimen/_8pxh"
                                android:layout_gravity="start"
                                android:layout_marginStart="@dimen/_124pxh"
                                android:layout_marginEnd="@dimen/_124pxh"
                                android:background="@drawable/rectangle_bg_deep_purple_900_75_radius_4" />

                            <View
                                android:id="@+id/viewRectangle30406"
                                android:layout_width="@dimen/_8pxh"
                                android:layout_height="@dimen/_8pxh"
                                android:layout_gravity="start"
                                android:layout_marginStart="@dimen/_62pxh"
                                android:layout_marginEnd="@dimen/_62pxh"
                                android:background="@drawable/rectangle_bg_white_a700_radius_4" />
                        </FrameLayout>
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:id="@+id/lineLine885"
                    android:layout_width="@dimen/_328pxh"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_gravity="end"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:background="@color/black_900_19" />

                <LinearLayout
                    android:id="@+id/linearColumnrectangle29401"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:gravity="start"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearRowrectangle29401"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:gravity="start"
                        android:orientation="horizontal">

                        <FrameLayout
                            android:id="@+id/frameStackrectangle29401"
                            android:layout_width="@dimen/_71pxh"
                            android:layout_height="@dimen/_71pxv"
                            android:layout_marginBottom="@dimen/_6pxv">

                            <ImageView
                                android:id="@+id/imageImage716"
                                imageRes="@{@drawable/img_image716}"
                                android:layout_width="@dimen/_71pxh"
                                android:layout_height="@dimen/_71pxh"
                                android:layout_gravity="start"
                                android:scaleType="fitXY"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_image716" />

                            <View
                                android:id="@+id/viewRectangle29401"
                                android:layout_width="@dimen/_71pxh"
                                android:layout_height="@dimen/_71pxh"
                                android:layout_gravity="start"
                                android:background="@drawable/rectangle_bg_indigo_a200_radius_6" />
                        </FrameLayout>

                        <LinearLayout
                            android:id="@+id/linearColumnverifyyouride"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.77"
                            android:gravity="start"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtVerifyYourIde"
                                style="@style/txtIntersemibold16"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@{identityProofVM.identityProofModel.txtProvideYourGo}"
                                tools:text="@string/msg_verify_your_ide" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@{identityProofVM.identityProofModel.txtProvideYourGo}"
                                tools:text="@string/msg_provide_your_go" />

                            <LinearLayout
                                android:id="@+id/linearRowcheckmark"
                                style="@style/groupStylegreen_900_63cornerRadius"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:gravity="start"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/imageCheckmark"
                                    android:layout_width="@dimen/_11pxh"
                                    android:layout_height="@dimen/_11pxh"
                                    android:layout_marginStart="@dimen/_7pxh"
                                    android:layout_marginTop="@dimen/_4pxv"
                                    android:layout_marginBottom="@dimen/_5pxv"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_checkmark"
                                    tools:ignore="ContentDescription"
                                    tools:src="@drawable/img_checkmark" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_5pxh"
                                    android:layout_marginTop="@dimen/_3pxv"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:ellipsize="end"
                                    android:lineSpacingExtra="@dimen/_3pxh"
                                    android:singleLine="true"
                                    android:text="@{identityProofVM.identityProofModel.txtDatasecur}"
                                    tools:text="@string/msg_100_data_secur" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <EditText
                        android:id="@+id/etGroup48103694"
                        style="@style/etRoundedOutline_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:background="@drawable/rectangle_border_gray_401_radius_7"
                        android:ellipsize="end"
                        android:hint="@string/lbl_aadhaar_number"
                        android:inputType="number"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_15pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_15pxv"
                        android:singleLine="true"
                        android:text="@={identityProofVM.identityProofModel.etGroup48103694Value}"
                        android:textColorHint="@color/gray_500"
                        tools:ignore="Autofill" />

                    <EditText
                        android:id="@+id/etGroup48103696"
                        style="@style/etRoundedOutline_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:background="@drawable/rectangle_border_gray_401_radius_7"
                        android:ellipsize="end"
                        android:hint="@string/lbl_pan_number"
                        android:inputType="text"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:paddingStart="@dimen/_16pxh"
                        android:paddingTop="@dimen/_15pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_15pxv"
                        android:singleLine="true"
                        android:text="@={identityProofVM.identityProofModel.etGroup48103696Value}"
                        android:textColorHint="@color/gray_500"
                        tools:ignore="Autofill" />
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>

        <ImageView
            android:id="@+id/center_landing_ph"
            android:layout_width="1dp"
            android:layout_height="1dp"
            android:layout_marginBottom="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/save_as_draft_btn"
            style="@style/Widget.MaterialComponents.Button.OutlinedButton.Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="40dp"
            android:insetLeft="0dp"
            android:insetTop="0dp"
            android:insetRight="0dp"
            android:insetBottom="0dp"
            android:padding="8dp"
            android:text="Save as Draft"
            android:textColor="#607CF2"
            app:iconGravity="textStart"
            app:iconPadding="0dp"
            app:iconSize="24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/center_landing_ph"
            app:layout_constraintStart_toStartOf="parent"
            app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.MyApp.Button.Circle"
            app:strokeColor="#607CF2" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/continue_btn"
            style="@style/Widget.MaterialComponents.Button.OutlinedButton.Icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="40dp"
            android:backgroundTint="#607CF2"
            android:insetLeft="0dp"
            android:insetTop="0dp"
            android:insetRight="0dp"
            android:insetBottom="0dp"
            android:padding="8dp"
            android:text="Continue"
            android:textColor="#FFFFFF"
            app:iconGravity="textStart"
            app:iconPadding="0dp"
            app:iconSize="24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/center_landing_ph"
            app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.MyApp.Button.Circle"
            app:strokeColor="#607CF2" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
